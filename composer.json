{
    "name": "uzbek/laravel-humo",
    "description": "Laravel api client for HUMO - \"National Interbank Processing Center\"",
    "keywords": [
        "uzbek",
        "laravel",
        "laravel-humo"
    ],
    "homepage": "https://github.com/uzbek/laravel-humo",
    "license": "MIT",
    "authors": [
        {
            "name": "Inoyatulloh",
            "email": "mronx@ya.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-simplexml": "*",
        "bilfeldt/laravel-http-client-logger": "^1.2",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-data": "^2.0",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Uzbek\\LaravelHumo\\": "src"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uzbek\\LaravelHumo\\LaravelHumoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
